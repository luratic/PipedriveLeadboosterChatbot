// Enter your template code here.
//const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const setInWindow = require('setInWindow');
const makeInteger = require('makeInteger');


//log("data", data);

const getPipedriveLeadboosterConfig = () => {
  const pipedriveLeadboosterConfig = copyFromWindow('pipedriveLeadboosterConfig');
  if (pipedriveLeadboosterConfig) {
    return pipedriveLeadboosterConfig;
  }
  setInWindow('pipedriveLeadboosterConfig',{         
     base: data.base,
     companyId: data.companyId,
     playbookUuid: data.playbookUuid,
     version: makeInteger(data.version)
  });
  return copyFromWindow('pipedriveLeadboosterConfig');
};

const pipedriveLeadboosterConfig = getPipedriveLeadboosterConfig();

const trackingUrl = 'https://leadbooster-chat.pipedrive.com/assets/loader.js';
injectScript(trackingUrl, data.gtmOnSuccess, data.gtmOnFailure, 'pipedriveLeadboosterChatbot');


